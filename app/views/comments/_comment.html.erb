<% rating = comment.user_question_feedback.ratings %>
<tr>
<td class="comment">
<% unless rating.nil? || rating[0].nil? %>
  <span class="rating"><%= rating[0].rating %></span>
<% end %>
<%= comment.comment.capitalize %></td>
</tr><tr>
<td class="timestamp">Posted by <span class="commenting_user"><%= comment.get_user.name %></span> <%= time_ago_in_words(comment.created_at) %> ago.</td>
</tr>